<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="SectionBigWrapPanel" TargetType="WrapPanel">
        <Setter Property="Margin" Value="5" />
        <Setter Property="Background" Value="AliceBlue"/>
    </Style>
    <Style x:Key="SectionStackPanel" TargetType="StackPanel">
        
    </Style>
    <Style x:Key="SectionTitleWithBottomBorder" TargetType="TextBlock">
        <Setter Property="FontSize" Value="20"/>
    </Style>
    <Style x:Key="TextBoxStyle" TargetType="TextBox">
        <Setter Property="BorderThickness" Value="0, 0, 0, 2"/>
        <Setter Property="Margin" Value="1,0,5,0"/>
    </Style>
</ResourceDictionary>